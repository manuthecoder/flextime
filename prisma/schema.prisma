generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Staff {
  id         Int     @id @default(autoincrement())
  department String? @db.VarChar(300)
  name       String? @db.VarChar(300)
  title      String? @db.VarChar(300)
  email      String? @db.VarChar(300)
}
